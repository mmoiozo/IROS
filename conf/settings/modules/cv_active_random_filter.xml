<!DOCTYPE settings SYSTEM "../settings.dtd">
<settings>
  <dl_settings>
    <dl_settings NAME="Filter">
      <dl_setting var="AR_FILTER_RND_PIX_SAMPLE"    min="1" step="100"      max="10000" 	module="computer_vision/active_random_filter" shortname="Sample size"/>    
      <dl_setting var="AR_FILTER_FLOOD_STYLE" 		min="0" step="1"		max="1" 		module="computer_vision/active_random_filter" shortname="Flood style" values="Omni|Clockwise"/>
      <dl_setting var="AR_FILTER_SAMPLE_STYLE" 		min="0" step="1"		max="2" 		module="computer_vision/active_random_filter" shortname="Sample style" values="Full|Grid|Random"/>
      <dl_setting var="AR_FILTER_Y_MIN"             min="0" step="1"        max="255"     	module="computer_vision/active_random_filter" shortname="Y min"/>
      <dl_setting var="AR_FILTER_Y_MAX"             min="0" step="1"        max="255"     	module="computer_vision/active_random_filter" shortname="Y max"/>
      <dl_setting var="AR_FILTER_U_MIN"             min="0" step="1"        max="255"     	module="computer_vision/active_random_filter" shortname="U min"/>
      <dl_setting var="AR_FILTER_U_MAX"             min="0" step="1"        max="255"     	module="computer_vision/active_random_filter" shortname="U max"/>
      <dl_setting var="AR_FILTER_V_MIN"             min="0" step="1"        max="255"     	module="computer_vision/active_random_filter" shortname="V min"/>
      <dl_setting var="AR_FILTER_V_MAX"             min="0" step="1"        max="255"     	module="computer_vision/active_random_filter" shortname="V max"/>
      <dl_setting var="AR_FILTER_CDIST_YTHRES"      min="0" step="1"        max="255"     	module="computer_vision/active_random_filter" shortname="dY"/>
      <dl_setting var="AR_FILTER_CDIST_UTHRES"      min="0" step="1"        max="255"     	module="computer_vision/active_random_filter" shortname="dU"/>
      <dl_setting var="AR_FILTER_CDIST_VTHRES"      min="0" step="1"        max="255"     	module="computer_vision/active_random_filter" shortname="dV"/>
      <dl_setting var="AR_FILTER_GREY_THRES"      	min="0" step="1"        max="255"     	module="computer_vision/active_random_filter" shortname="Grey threshold"/>
      
      <dl_setting var="AR_FILTER_MIN_CIRCLE_PERC"   min="0" step="0.01"     max="1"    		module="computer_vision/active_random_filter" shortname="Min fill (%)"/>
      <dl_setting var="AR_FILTER_MAX_CIRCLE_DEF"    min="0" step="0.001" 	max="1"     	module="computer_vision/active_random_filter" shortname="Max eccentricity"/>  
      <dl_setting var="AR_FILTER_CAM_RANGE"      	min="0" step="0.01"     max="10"     	module="computer_vision/active_random_filter" shortname="max range (m)"/>
       <dl_setting var="default_calArea"   			min="1" step="1"        max="30000"    	module="computer_vision/active_random_filter" shortname="calArea"/>
  </dl_settings>
  <dl_settings NAME="Stabilization">     
      <dl_setting var="AR_FILTER_VIEW_R" 			min="0" step="0.000001" max="5"     	module="computer_vision/active_random_filter" shortname="p dist"/>  
      <dl_setting var="default_k"    				min="1" step="0.0000001"	max="2"    	module="computer_vision/active_random_filter" shortname="k"/>  
      <dl_setting var="default_6th_o"    			min="0" step="0.001"	max="25"    	module="computer_vision/active_random_filter" shortname="6th order coef"/> 
      <dl_setting var="default_2nd_o"    			min="0" step="0.001"	max="25"  		module="computer_vision/active_random_filter" shortname="2nd order coef"/> 
      <dl_setting var="angleOfView"    				min="0" step="0.001" 	max="180"    	module="computer_vision/active_random_filter" shortname="angle of view"/>      
    
    </dl_settings>
  </dl_settings>
</settings>
